.main{
	width: 100%;
	display: inline-block;
	background: #f7f7f7;
  padding-bottom:50px;
}
.main .wechat{
	width: 100%;
	float: left;
	background: #44a2e2;
  padding: 20px 20px;
  position: relative;
}
.main .wechat .wechat-pic{
  position: absolute;
  top: 16px;
  right: 20px;
  width: 60px;
  height: 60px;
  border-radius: 50%;
  overflow: hidden;
}
.main .wechat-name{
  width: 100%;
  float: left;
  font-size: 18px;
  color: #fff;
}
.main .wechat-list {
  width: 100%;
  float: left;
  font-size: 13px;
  color: rgb(163,225,252);
  margin-top: 2px;
}
.main .wechat-list image{
  width: 42px;
  height: 34px;
  display: inline-block;
  position: relative;
  top: 8px;
  margin-right: 2px;
}
.main .wechat-list .wechat-gradeMs{
  font-size: 14px;
}
.main .wechat-gradeList {
  width: 100%;
  float: left;
  margin-top: 12px;
}
.main .wechat-gradeList .wechat-gradeList-title{
  width: 80px;
  float: left;
  color: #fff;
  font-size: 14px;
  line-height: 54px;
}
.main .wechat-gradeList .wechat-gradeList-list{
  width: calc(100% - 80px);
  float: right;
  padding: 0 24px;
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-top{
  width: 100%;
  height: 18px;
  display: inline-block;
  position: relative;
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-top view{
  /* width: 33.33%; */
  float: left;
  text-align: right;
  position: absolute;
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-top view image{
  width: 24px;
  height: 18px;
  position: absolute;
  right: -4px;
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-jd{
  width: 100%;
  height: 8px;
  border-radius: 150px;
  background: rgb(0,113,188);
  position: relative;
  overflow: hidden;
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-jd view{
  width: 0;
  height: 100%;
  position: absolute;
  /* border-radius: 150px; */
  top: 0;
  left: 0;
  background: rgb(147,228,249);
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-text{
    width: 100%;
    line-height: 20px;
    color: #fff;
    position: relative;
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-text text{
  width: calc(100% / 3);
  float: left;
  font-size: 12px;
  padding-top: 4px;
  position: absolute;
  text-align: right;
  padding-right: -12px;
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-text text:first-child{
  left: 0;
  width: auto;
}
.main .wechat-gradeList .wechat-gradeList-list .gradeList-list-text text:last-child{
  position: absolute;
  top: 0;
  width: auto;
  right: -12px;
}
.main .infromlist{
	width: 100%;
	display: inline-block;
}
.main .infromlist .ul{
	width: 100%;
	display: inline-block;
	margin-top: 6vw;
}
.main .infromlist .ul .li{
	width: 100%;
	display: inline-block;
	background: #fff;
	padding: 12px;
}
.main .infromlist .ul .li .h2{
	font-size: 1.3em;
	float: left;
}
.main .infromlist .ul .li .icon{
	float: right;
	color: #999;
}
