/* 获取用户信息授权 */
.getUserInfo{
  width: 94vw;
  position: fixed;
  top: 28vh;
  left: 3vw;
  background: #fff;
  border-radius: 4px;
  padding: 16px;
  z-index: 9;
}
.getUserInfo .userInfoTips{
  width: 100%;
  display: inline-block;
  margin-bottom: 12px;
}
.getUserInfo .userInfoTips image{
  width: 46px;
  height: 46px;
  float: left;
}
.getUserInfo .userInfoTips text {
  width: calc(100% - 46px);
  float: left;
  padding-left: 12px;
  box-sizing: border-box;
}
.getUserInfo .userInfoTips text.tipsTop{
  font-size: 16px;
  color: #666666;
}
.getUserInfo .userInfoTips text.tipsP{
  font-size: 12px;
  color: #999999;
  margin-top: 2px;
}
.getUserInfo .grandBtn{
  width: 90%;
  float: left;
  margin: 0 5%;
  height: 42px;
  font-size: 16px;
  border-radius: 4px;
  color: #ffffff;
  background: rgb(26,175,25);
}
.getUserInfo-bg{
  width: 100vw;
  height: 100vh;
  position: fixed;
  z-index: 8;
  background: rgba(0,0,0,0.5);
}
.productInfo{
  text-align: center;
  font-size: 1.2em;

}
.sendInfo{
  text-align: center;
  font-size: 1.2em
}